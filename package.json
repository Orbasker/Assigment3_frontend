{
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint ."
    ]
  },
  "scripts": {
    "start": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "start-server": "node index.js",
    "start-client": "npm start --prefix emergency-supply-app",
    "precommit": "pre-commit run --all",
    "build": "npm run build --prefix emergency-supply-app"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mongoose": "^8.1.3",
    "precommit": "^1.2.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@heroicons/react": "^2.1.1",
    "concurrently": "^8.2.2"
  }
}
